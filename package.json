    "gatsby-transformer-remark": "^6.12.0",
    "gatsby-transformer-sharp": "^4.2.0",
    "github-slugger": "^1.4.0",
    "performant-array-to-tree": "^1.9.1",
    "prism-react-renderer": "^1.3.1",
    "prismjs": "^1.25.0",
    "prop-types": "^15.7.2",
    "query-string": "^7.0.1",
